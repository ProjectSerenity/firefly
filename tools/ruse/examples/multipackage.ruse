(package main)

(import
	("firefly-os.dev/tools/ruse/examples/printer"))

'(arch x86-64)
(asm-func main
	(mov rsi (string-pointer printer.Default-message))
	(mov rdx (len printer.Default-message))
	(call (func printer.Print))

	(mov eax 60)   ; sys_exit
	(xor rdi rdi)  ; int error_code
	(syscall))     ; exit(0)
